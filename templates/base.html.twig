<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Art et création en partage
			{% endblock %}
		</title>
		<link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>"> {% block stylesheets %}{% endblock %}{% block javascripts %}{% endblock %}
		</head>
		<div>
			<!-- Navigation -->
			{% include "inc/nav.html.twig" %}
			<!-- Main Content -->
			<div>
				{% block body %}{% endblock %}

				{% for key, messages in app.flashes %}
					{% for message in messages %}
						<div class="alert alert-{{ key }}">
							{{ message }}
						</div>
					{% endfor %}
				{% endfor %}
			</div>
			<!-- Footer -->

		</div>
	</html>
